  <Requirements:Requirement xmi:id="_CW8f4NB4EeeBrIZE6W0rvQ" base_Class="_CWkscNB4EeeBrIZE6W0rvQ" id="NR2" text="El drone debe garantizar la seguridad de las personas"/>
  <Requirements:Requirement xmi:id="_pK6qYNB5EeeBrIZE6W0rvQ" base_Class="_pKpkoNB5EeeBrIZE6W0rvQ" id="VNR2" text="El drone debe volar a una altura suficientemente alta para evitar colisionar con las personas"/>
